	<queryString>
		<![CDATA[SELECT
     ordre_payement."beneficiaire" AS ordre_payement_beneficiaire,
     ordre_payement."montant" AS ordre_payement_montant,
     ordre_payement."montant_en_lettre" AS ordre_payement_montant_en_lettre,
     ordre_payement."pieces" AS ordre_payement_pieces,
     ordre_payement."date_ordonnation" AS ordre_payement_date_ordonnation,
     ordre_payement."code" AS ordre_payement_code,
     boncommande."objet" AS boncommande_objet,
     boncommande."code" AS boncommande_code,
     boncommande."ttc" AS boncommande_ttc,
     boncommande."espece" AS boncommande_espece,
     boncommande."cheque" AS boncommande_cheque,
     boncommande."virement" AS boncommande_virement,
     boncommande."montant_ordonne" AS boncommande_montant_ordonne,
     boncommande."taxe_ordonne" AS boncommande_taxe_ordonne,
     boncommande."nap_ordonne" AS boncommande_nap_ordonne,
     budget."annee" AS budget_annee,
     structure."nom" AS structure_nom,
     institution."nom" AS institution_nom,
     institution."nom_en" AS institution_nom_en,
     boncommande."imputations" AS boncommande_imputations,
     signataire."nom" AS signataire_nom,
     signataire."prenom" AS signataire_prenom,
     ordre_payement."objet" AS ordre_payement_objet,
     boncommande."totalht" AS boncommande_totalht,
     boncommande."tva" AS boncommande_tva,
     boncommande."totalair" AS boncommande_totalair,
     societe."nom" AS societe_nom,
     societe."rib1" AS societe_rib1,
     ordre_payement."mode_paiement" AS ordre_payement_mode_paiement
FROM
     "public"."boncommande" boncommande INNER JOIN "public"."ordre_payement" ordre_payement ON boncommande."idboncommande" = ordre_payement."idboncommande"
     INNER JOIN "public"."budget" budget ON boncommande."idbudget" = budget."idbudget"
     INNER JOIN "public"."structure" structure ON boncommande."idstructure" = structure."idstructure"
     INNER JOIN "public"."signataire" signataire ON boncommande."idsignataire" = signataire."idsignataire"
     INNER JOIN "public"."societe" societe ON boncommande."idsociete" = societe."idsociete"
     INNER JOIN "public"."institution" institution ON structure."idinstitution" = institution."idinstitution"
WHERE
     ordre_payement."id_ordre_payement" = $P{idOp}]]>
	</queryString>
